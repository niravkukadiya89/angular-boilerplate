{
  "name": "angularboilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "minifyvendor": "uglifyjs dist/vendor.js --screw-ie8 --compress --mangle --output dist/vendor.js",
    "minifyMain": "uglifyjs dist/main.js --screw-ie8 --compress --mangle --output dist/main.js",
    "minifyPolyfill": "uglifyjs  dist/polyfills.js  --screw-ie8 --compress --mangle --output dist/polyfills.js",
    "minifyScript": "uglifyjs  dist/scripts.js  --screw-ie8 --compress --mangle --output dist/scripts.js",
    "minifyRuntime": "uglifyjs  dist/runtime.js --screw-ie8 --compress --mangle --output dist/runtime.js",
    "minifyAll": "npm run minifyvendor && npm run minifyMain && npm run minifyPolyfill && npm run minifyScript && npm run minifyRuntime",
    "build": "ng build --extract-css && npm run minifyAll",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.3.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.3.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-font-awesome": "^3.1.2",
    "angular-svg-icon": "^7.0.0",
    "angular2-datepicker": "^1.0.5",
    "angular2-datetimepicker": "^1.1.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "material-design-icons": "^3.0.1",
    "ng": "0.0.0",
    "ng2-search-filter": "^0.4.7",
    "ngx-bootstrap": "^3.2.0",
    "ngx-toastr": "^9.1.0",
    "node-minify": "^3.4.0",
    "places.js": "^1.15.1",
    "popper.js": "^1.14.4",
    "primeicons": "^1.0.0",
    "rxjs": "^6.0.0",
    "tslint-no-unused": "^0.2.0-alpha.1",
    "xlsx": "^0.14.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.1",
    "@angular/cli": "^7.3.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-consistent-codestyle": "^1.13.3",
    "tslint-defocus": "^2.0.3",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-origin-ordered-imports-rule": "^1.1.2",
    "tslint-rxjs-subject-restrictions-rule": "^1.0.4",
    "typescript": "~3.1.1"
  }
}
